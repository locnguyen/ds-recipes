{
    "presets": [
        "es2015",
        "stage-0",
        "react",
        "mocha"
    ],
    "plugins": ["transform-object-rest-spread", "transform-es2015-destructuring", "transform-async-to-generator"],
    "env": {
        "development": {
            "plugins": [
                "react-hot-loader/babel"
            ]
        },
        "production": {
            "presets": [
                "react-optimize"
            ]
        }
    }
}
